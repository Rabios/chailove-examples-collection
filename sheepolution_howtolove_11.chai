// Don't forget to load the file
require("rectangle")
global r1
global r2

def load() {
  r1 = Rectangle(100, 100, 200, 50)
  r2 = Rectangle(350, 80, 25, 140)
}

def update(dt) {
  r1.update(dt)
  r2.update(dt)
}

def draw() {
  r1.draw()
  r2.draw()
}