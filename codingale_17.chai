// Example: Optimizing images

global angle = 0
global image
global x
global y

def load() {
  image = love.graphics.newImage("assets/love-ball.png")
}

def update(dt) {
  angle = angle + dt
  x = 400 + cos(angle) * 100
  y = 300 + sin(angle) * 100
}

def draw() {
  love.graphics.draw(image, x, y)
}