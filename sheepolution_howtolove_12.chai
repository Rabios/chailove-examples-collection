// Don't forget to load the file
require("rectangle")
require("circle")
global r1
global r2
  
def load() {
  r1 = Rectangle(100, 100, 200, 50)
  r2 = Circle(350, 80, 40)
}

def update(dt) {
  r1.update(dt)
  r2.update(dt)
}

def draw() {
  r1.draw()
  r2.draw()
}